        <Label x:Name="label_Problem" Content="Parametry problemu " HorizontalAlignment="Left" Margin="114,590,0,0" VerticalAlignment="Top" FontWeight="Bold" FlowDirection="LeftToRight" RenderTransformOrigin="1.702,0.526"/>
